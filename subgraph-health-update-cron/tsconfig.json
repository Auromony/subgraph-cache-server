{
  "compilerOptions": {
    "outDir": "health-update-cron/dist",
    "module": "commonjs",
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "types": [
      "@cloudflare/workers-types",
      "@types/jest",
      "@types/service-worker-mock"
    ]
  },
  "include": ["store-update-cron/src"],
  "exclude": ["node_modules", "health-ustore-update-cron/teste-cron/dist", "test"],
  "typeAcquisition": { "enable": true }

}
